    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// Normalización de datos para Intelite AI\n// Mapeamos los datos del RSS a nuestro esquema interno\n\n// Intentamos limpiar el resumen de etiquetas HTML si existen\nconst summaryRaw = $json.contentSnippet || $json.content || \"\";\nconst summaryClean = summaryRaw.replace(/<[^>]*>?/gm, '');\n\nreturn {\n  source_name: \"TechCrunch AI\",\n  article_title: $json.title,\n  article_url: $json.link,\n  article_summary: summaryClean.substring(0, 500) + \"...\", // Limitamos longitud\n  pub_date: $json.pubDate || $json.isoDate,\n  // Guardamos el ID único (guid) para usarlo luego en la deduplicación\n  article_guid: $json.guid || $json.link\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
